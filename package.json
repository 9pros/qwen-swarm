{
  "name": "qwen-swarm",
  "version": "1.0.0",
  "description": "Qwen Code-based swarm orchestration system with self-improving and self-healing capabilities",
  "main": "dist/index.js",
  "bin": {
    "qwen-swarm": "dist/cli/main.js",
    "qwen-swarm-enhanced": "dist/cli/enhanced-main.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "cli": "tsx src/cli/main.ts",
    "cli:enhanced": "tsx src/cli/enhanced-main.ts",
    "build:cli": "tsc && tsc-alias && chmod +x dist/cli/main.js",
    "build:enhanced": "tsc && tsc-alias && chmod +x dist/cli/enhanced-main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "jest --testPathPattern=performance",
    "test:chaos": "jest --testPathPattern=chaos",
    "test:security": "jest --testPathPattern=security",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:regression": "jest --testPathPattern=regression",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "quality:check": "npm run lint && npm run type-check && npm run test:ci",
    "security:audit": "npm audit --audit-level=moderate",
    "security:snyk": "snyk test",
    "precommit": "npm run quality:check",
    "db:test:setup": "tsx scripts/test-db-setup.ts",
    "db:test:teardown": "tsx scripts/test-db-teardown.ts",
    "perf:benchmark": "node --experimental-modules scripts/benchmark.js",
    "chaos:simulate": "tsx scripts/chaos-simulator.ts",
    "agents:list": "tsx src/cli/agents.ts list",
    "agents:install": "tsx src/cli/agents.ts install",
    "agents:uninstall": "tsx src/cli/agents.ts uninstall",
    "agents:update": "tsx src/cli/agents.ts update",
    "agents:search": "tsx src/cli/agents.ts search",
    "agents:info": "tsx src/cli/agents.ts info",
    "agents:init": "tsx src/cli/agents.ts init",
    "agents:cleanup": "tsx src/cli/agents.ts cleanup",
    "agents:stats": "tsx src/cli/agents.ts stats",
    "agents:registry:add": "tsx src/cli/agents.ts registry add",
    "agents:registry:remove": "tsx src/cli/agents.ts registry remove",
    "agents:registry:list": "tsx src/cli/agents.ts registry list",
    "agents:registry:update": "tsx src/cli/agents.ts registry update"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^8.7.0",
    "zod": "^3.22.4",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "eventemitter3": "^5.0.1",
    "piscina": "^4.2.1",
    "node-cron": "^3.0.3",
    "commander": "^11.1.0",
    "chokidar": "^3.5.3",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "open": "^9.1.0",
    "glob": "^10.3.10",
    "axios": "^1.6.2",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.20.0",
    "semver": "^7.5.4",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "tsx": "^4.6.2",
    "tsc-alias": "^1.8.8",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.9",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/node-cron": "^3.0.11",
    "@types/js-yaml": "^4.0.9",
    "@types/semver": "^7.5.6",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "jest-mock-extended": "^3.0.6",
    "jest-environment-node": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "playwright": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "mongodb-memory-server": "^9.1.6",
    "testcontainers": "^10.7.2",
    "faker": "^6.6.6",
    "@types/faker": "^6.6.9",
    "artillery": "^2.0.3",
    "clinic": "^12.1.0",
    "autocannon": "^7.15.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "snyk": "^1.1264.0",
    "benchmark": "^2.1.4",
    "@types/benchmark": "^2.1.5",
    "@types/inquirer": "^9.0.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}